<script>
    $(document).ready(function () {

    });

    var showEmailId = 1;
    function addEmail() {
        $("#emailDiv" + showEmailId).show();
        showEmailId++;

        if (showEmailId >= 10) {
            $("#addEmailDiv").hide();
        }
    }

    var showPhoneId = 1;
    function addPhone() {
        $("#phoneDiv" + showPhoneId).show();
        showPhoneId++;

        if (showPhoneId >= 10) {
            $("#addPhoneDiv").hide();
        }
    }

    var showSubContactId = 1;
    function addSubContact() {
        $("#subcontact" + showSubContactId).show();
        showSubContactId++;

        if (showSubContactId > 2) {
            $("#addSubContactDiv").hide();
        }
    }
</script>