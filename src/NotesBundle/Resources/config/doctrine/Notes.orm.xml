<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="NotesBundle\Entity\Notes" table="notes">
    <id name="id" type="integer" column="id" >
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="true"/>
    <field name="priority" type="string" column="priority" length="50" nullable="true"/>
    <field name="due" type="string" column="due" length="50" nullable="true"/>
    <field name="visibility" type="string" column="visibility" length="50" nullable="true"/>
    <field name="notes" type="string" column="notes" length="1000" nullable="false"/>
    <field name="tags" type="string" column="tags" length="500" nullable="true"/>
    <field name="type" type="string" column="type" length="50" nullable="false"/>
    <field name="typeid" type="integer" column="typeid" nullable="false"/>
    <field name="username" type="string" column="username" length="50" nullable="false"/>
     <field name="fullname" type="string" column="fullname" length="100" nullable="false"/>
    <field name="timestamp" type="string" column="timestamp" length="100" nullable="false"/>
    <field name="status" type="string" column="status" length="50" nullable="false"/>
    <field name="companyname" type="string" column="companyname" length="50" nullable="false"/>
    
  </entity>
</doctrine-mapping>
